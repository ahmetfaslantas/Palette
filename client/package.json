    "lint:js": "eslint . --ext .js,.jsx",
    "lint:css": "stylelint **/*.css",
    "lint:js:fix": "eslint . --ext .js,.jsx --fix",
    "lint:css:fix": "stylelint **/*.css --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
