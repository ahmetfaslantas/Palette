{
  "name": "palette",
  "version": "1.0.0",
  "description": "A simple LMS built with the MERN stack.",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "start": "node src/index.js",
    "start-watch": "nodemon src/index.js",
    "dev": "concurrently \"npm run start-watch\" \"cd client && npm start\"",
    "postinstall": "cd client && npm install",
    "clean": "rm -rf node_modules client/node_modules client/build"
  },
  "keywords": [
    "LMS"
  ],
  "author": "Ahmet Faruk ASLANTAÅž",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.4"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "eslint": "^8.10.0",
    "eslint-plugin-react": "^7.29.4",
    "nodemon": "^2.0.15"
  }
}
